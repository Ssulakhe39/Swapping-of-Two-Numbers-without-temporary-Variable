#include<stdio.h>
main()
{
	int x,y;
	printf("Enter value of x and y: ");
	scanf("%d",&x);
	printf("\n" "x value given is: " "%d",x);
	scanf("%d",&y);
	printf("\n""y value given is: " "%d",y);
	x=x+y;
	y=x-y;
	x=x-y;
	printf("\n" "After Swapping x is: " "%d",x);
	printf("\n" "After Swapping y is: " "%d",y);
	return 0;
}
